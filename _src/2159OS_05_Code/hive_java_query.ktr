        <class_source><![CDATA[import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.ResultSetMetaData;
import java.sql.SQLException;
import java.sql.Statement;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws Exception { 

    Object[] rowd = getRow();
    if (rowd == null) {
        setOutputDone();
        return false;
    }

	String sql = "select exchange, stock_symbol, avg(stock_price_open) as stock_price_open,  avg(stock_price_high) as stock_price_high,  avg(stock_price_low) as stock_price_low,  avg(stock_price_close) as stock_price_close,  avg(stock_volume) as stock_volume,  avg(stock_price_adj_close) as stock_price_adj_close,  month, year from nyse_stocks where stock_symbol in ('ARM','BBX','DAI','ROS') group by exchange, stock_symbol, month, year";
	Integer hasil;

    try {
      Class.forName("org.apache.hive.jdbc.HiveDriver");
    } catch (ClassNotFoundException e) {
      e.printStackTrace();
      throw e;
    }

	try
	{
    	Connection con = DriverManager.getConnection("jdbc:hive2://192.168.1.122:10000/default", "", "");
	    Statement stmt = con.createStatement();
		
    	ResultSet res = stmt.executeQuery(sql);

		Object[] out_row = RowDataUtil.createResizedCopy(rowd, data.outputRowMeta.size()+2);

	    while (res.next()) {
		   get(Fields.Out, "exchange").setValue(out_row, res.getString(1));
		   get(Fields.Out, "stock_symbol").setValue(out_row, res.getString(2));
		   get(Fields.Out, "stock_price_open").setValue(out_row, res.getDouble(3));
		   get(Fields.Out, "stock_price_high").setValue(out_row, res.getDouble(4));
		   get(Fields.Out, "stock_price_low").setValue(out_row, res.getDouble(5));
		   get(Fields.Out, "stock_price_close").setValue(out_row, res.getDouble(6));
		   get(Fields.Out, "stock_volume").setValue(out_row, res.getDouble(7));
		   get(Fields.Out, "stock_price_adj_close").setValue(out_row, res.getDouble(8));
		   get(Fields.Out, "month").setValue(out_row, res.getLong(9));
		   get(Fields.Out, "year").setValue(out_row, res.getLong("year"));
		   //Long year = get(Fields.Out, "year").getInteger(r);
		   //hasil = res.getInt("year");
    	   putRow(data.outputRowMeta, out_row);
	    }
    }
	catch(SQLException e)
	{
      e.printStackTrace();
      throw e;
	}

   return true;
}]]></class_source>
